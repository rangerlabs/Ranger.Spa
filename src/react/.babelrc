{
    "presets": [
        "@babel/preset-react",
        "@babel/preset-typescript",
        [
            "@babel/preset-env",
            {
                "targets": {
                    "browsers": ["last 2 versions"]
                },
                "modules": false
            }
        ]
    ],
    "plugins": [
        ["react-hot-loader/babel"],
        [
            "babel-plugin-import",
            {
                "libraryName": "@material-ui/icons",
                "libraryDirectory": "esm",
                "camel2DashComponentName": false
            }
        ]
    ]
}
